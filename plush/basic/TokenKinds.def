#ifndef PLUSH_BINOP
#define PLUSH_BINOP(...)
#endif // PLUSH_BINOP

#ifndef PLUSH_PUNCTUATOR
#define PLUSH_PUNCTUATOR(...)
#endif // PLUSH_PUNCTUATOR

#ifndef PLUSH_KEYWORD
#define PLUSH_KEYWORD(...)
#endif // PLUSH_KEYWORD

PLUSH_BINOP(LPIPE, "<|", RASSOC, 0)
PLUSH_BINOP(RPIPE, "|>", LASSOC, 1)

PLUSH_PUNCTUATOR(LPAREN, "(")
PLUSH_PUNCTUATOR(RPAREN, ")")
PLUSH_PUNCTUATOR(LBRACK, "[")
PLUSH_PUNCTUATOR(RBRACK, "]")
PLUSH_PUNCTUATOR(LCURLYBRACK, "{")
PLUSH_PUNCTUATOR(RCURLYBRACK, "}")
PLUSH_PUNCTUATOR(COLON, ":")
PLUSH_PUNCTUATOR(SEMICOLON, ";")

PLUSH_KEYWORD(IF, "if")
PLUSH_KEYWORD(DO, "do")
PLUSH_KEYWORD(LET, "let")
PLUSH_KEYWORD(MODULE, "module")
PLUSH_KEYWORD(IMPORT, "import")
PLUSH_KEYWORD(AS, "as")

#undef PLUSH_BINOP
#undef PLUSH_PUNCTUATOR
#undef PLUSH_KEYWORD
